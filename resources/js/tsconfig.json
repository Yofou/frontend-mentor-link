{
  "include": ["**/*", "styled-system"],
  "compilerOptions": {
    "lib": ["DOM"],
    "jsx": "react",
    "sourceMap": true,
    "esModuleInterop": true,
    "paths": {
      "App/*": [
        "../../app/*"
      ],
      "Config/*": [
        "../../config/*"
      ],
      "Contracts/*": [
        "../../contracts/*"
      ],
      "Database/*": [
        "../../database/*"
      ],
      "styled-system": ["./styled-system/*"],
      "styled-system/*": ["./styled-system/*"]
    },
  }
}
